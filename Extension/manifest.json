{
    "name": "Streaming Service Standings",
    "description": "An extension to add Streaming Service Ratings",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "notifications",
        "scripting",
        "tabs"
    ]
  }